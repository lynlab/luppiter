{
  "name": "luppiter",
  "version": "1.0.0",
  "repository": "https://github.com/hellodhlyn/luppiter",
  "author": "Do Hoerin <hellodhlyn@gmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -r ./dist || true",
    "lint": "eslint -c .eslintrc.js src/**/*.ts",
    "test": "NODE_ENV=test mocha -r ts-node/register -r source-map-support/register -r test/prepare.ts --fule-trace --bail --reporter dot",
    "test:all": "NODE_ENV=test nyc mocha -r ts-node/register -r source-map-support/register -r test/prepare.ts --fule-trace --bail --reporter dot 'test/**/*.spec.ts'",
    "coverage": "nyc report --reporter=json",
    "prebuild": "yarn clean",
    "build": "tsc",
    "prestart": "yarn lint; yarn build",
    "start": "node .",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js --config src/ormconfig.ts",
    "typeorm:migrate": "yarn typeorm migration:run",
    "typeorm:revert": "yarn typeorm migration:revert",
    "typeorm:show": "yarn typeorm migration:show"
  },
  "dependencies": {
    "@grpc/proto-loader": "^0.5.1",
    "@msgpack/msgpack": "^1.11.0",
    "aws-sdk": "^2.480.0",
    "axios": "^0.19.0",
    "cors": "^2.8.5",
    "dockerode": "^3.0.2",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.5",
    "express-http-context": "^1.2.2",
    "file-type": "^12.0.0",
    "grpc": "^1.24.2",
    "pg": "^7.17.1",
    "query-string": "^6.10.1",
    "typeorm": "^0.2.18",
    "typeorm-naming-strategies": "^1.1.0",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/chai-http": "^4.2.0",
    "@types/cors": "^2.8.5",
    "@types/dockerode": "^2.5.20",
    "@types/express": "^4.17.0",
    "@types/express-fileupload": "^1.1.0",
    "@types/faker": "^4.1.5",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.0.7",
    "@types/protobufjs": "^6.0.0",
    "@types/query-string": "^6.3.0",
    "@types/rosie": "^0.0.36",
    "@types/sinon": "^7.5.1",
    "@types/uuid": "^3.4.5",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.0",
    "faker": "^4.1.0",
    "mocha": "^7.0.0",
    "mocha-prepare": "^0.1.0",
    "nyc": "^15.0.0",
    "rosie": "^2.0.1",
    "sinon": "^8.1.0",
    "source-map-support": "^0.5.12",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/migrations/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "all": true
  }
}
