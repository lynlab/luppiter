{
  "name": "luppiter",
  "version": "1.0.0",
  "repository": "https://github.com:lynlab/luppiter",
  "author": "Do Hoerin <hellodhlyn@gmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -r ./dist || true",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "test": "mocha -r ts-node/register -r source-map-support/register -r test/prepare.ts --fule-trace --bail --reporter dot",
    "test:all": "nyc mocha -r ts-node/register -r source-map-support/register -r test/prepare.ts --fule-trace --bail --reporter dot 'test/**/*.spec.ts'",
    "coverage": "nyc report --reporter=json",
    "prebuild": "yarn clean",
    "build": "tsc",
    "prestart": "yarn lint; yarn build",
    "start": "node .",
    "typeorm": "node ./node_modules/typeorm/cli.js",
    "seed": "TYPEORM_MIGRATIONS=dist/seeds/*.js yarn typeorm migration:run"
  },
  "dependencies": {
    "@grpc/proto-loader": "^0.5.1",
    "@msgpack/msgpack": "^1.6.0",
    "@types/cors": "^2.8.5",
    "@types/dockerode": "^2.5.20",
    "@types/dotenv": "^6.1.1",
    "@types/express-fileupload": "^1.1.0",
    "@types/protobufjs": "^6.0.0",
    "@types/query-string": "^6.3.0",
    "@types/uuid": "^3.4.5",
    "aws-sdk": "^2.480.0",
    "axios": "^0.19.0",
    "cors": "^2.8.5",
    "dockerode": "^2.5.8",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.5",
    "express-http-context": "^1.2.2",
    "file-type": "^12.0.0",
    "grpc": "^1.22.2",
    "pg": "^7.11.0",
    "query-string": "^6.8.1",
    "typeorm": "^0.2.18",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/chai-http": "^4.2.0",
    "@types/express": "^4.17.0",
    "@types/faker": "^4.1.5",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.0.7",
    "@types/rosie": "^0.0.36",
    "@types/sinon": "^7.0.13",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "faker": "^4.1.0",
    "mocha": "^6.1.4",
    "mocha-prepare": "^0.1.0",
    "nyc": "^14.1.1",
    "rosie": "^2.0.1",
    "sinon": "^7.3.2",
    "source-map-support": "^0.5.12",
    "ts-node": "^8.3.0",
    "tslint": "^5.17.0",
    "typescript": "^3.5.1"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "all": true
  }
}
